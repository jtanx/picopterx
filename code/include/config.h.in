/**
 * @file config.h
 * @brief System-specific and compile-time specific configuration parameters.
 */
 
#ifndef _PICOPTERX_CONFIG_H
#define _PICOPTERX_CONFIG_H

#define PICOPTER_VERSION "${PICOPTER_VERSION}"
#define PICOPTER_DATE "${PICOPTER_DATE}"

/* Home location */
#define PICOPTER_HOME_LOCATION "${PICOPTER_HOME_LOCATION}"

/* Log location */
#define PICOPTER_LOG_LOCATION "${PICOPTER_HOME_LOCATION}/logs"

/* Do we use syslog? */
#cmakedefine USE_SYSLOG

/* Are we running on the pi? */
#cmakedefine IS_ON_PI

/* Are we using the real libgps? */
#cmakedefine IS_REAL_LIBGPS

#endif // _PICOPTERX_CONFIG_H